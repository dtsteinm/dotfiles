begin_custom_css
.__vim_hint_highlight_span {
font-size: 28px
}
end_custom_css

" Faster scrolling
map <C-h> h
map <C-j> j
map <C-k> k
map <C-l> l
map h 15h
map j 10j
map k 10k
map l 15l

" Quickly jump between tabs
map J gT
map K gt

" Close current tab
map x dc

" Select last active tab
map g; <C-^>

" Use letters in Hint mode
set useletters=1

" Hint keys focused on home row
set hintkeys=jlkhfsdag,werui

" Don't load on these sites
set disablesites=mail.google.com

" Additional search engines to use
set searchengines+={"ann":"http://www.animenewsnetwork.com/encyclopedia/search.php?searchbox={{keyword}}", "cdj":"http://www.cdjapan.co.jp/search3.html?q={{keyword}}&media=&r=any&step=20&order=score", "cr":"http://www.crunchyroll.com/search?q={{keyword}}", "dict":"http://dictionary.reference.com/search?q={{keyword}}", "dvd":"http://www.dvdaf.com/search.html?has={{keyword}}&rgn=us&init_form=str0_0_has_{{keyword}}*rgn_0_us", "fb":"http://www.facebook.com/search/?src=os&q={{keyword}}", "fp":"http://www.fandompost.com/?s={{keyword}}&x=0&y=0", "git":"https://github.com/search?q={{keyword}}", "goo":"http://www.google.com/search?q={{keyword}}", "li":"http://www.linkedin.com/search/fpsearch?type=people&keywords={{keyword}}&pplSearchOrigin=GLHD&pageKey=member-home&search=Search", "the":"http://thesaurus.com/browse/{{keyword}}", "trsi":"http://www.rightstuf.com/rssite/search/?SearchTerms={{keyword}}&image=Search", "wiki":"http://en.wikipedia.org/wiki/Special:Search?search={{keyword}}", "yt":"https://www.youtube.com/results?search_query={{keyword}}&search=Search"} 

